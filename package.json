{
  "name": "vue-tv",
  "version": "1.1.2",
  "description": "Spatial navigation library based on Vue js",
  "author": "Jazzie-Jay",
  "keywords": [
    "vue",
    "spatial-navigation",
    "smarttv",
    "navigation",
    "key-navigation",
    "tv",
    "vue-tv"
  ],
  "homepage": "https://github.com/Jazzie-z/vue-spatial-navigation/tree/npm",
  "repository": {
    "type": "git",
    "url": "git@github.com/Jazzie-z/vue-spatial-navigation/tree/npm"
  },
  "main": "dist/vue-spatial-navigation.ssr.js",
  "browser": "dist/vue-spatial-navigation.esm.js",
  "module": "dist/vue-spatial-navigation.esm.js",
  "unpkg": "dist/vue-spatial-navigation.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "src/**/*.js"
  ],
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "mitt": "^2.1.0",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "cross-env": "^7.0.2",
    "minimist": "^1.2.5",
    "rollup": "^2.7.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^5.1.6",
    "vue-router": "^3.4.5",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "engines": {
    "node": ">=10"
  }
}
